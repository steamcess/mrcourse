<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>List Course</title>
</head>
<body>
    <section>
        <table border="1">
            <thead>
                <tr>
                    <th>No</th>
                    <th>Course Name</th>
                    <th>Price</th>
                    <th>Action</th>
                </tr>
            </thead>

            <tbody>
                <% courses.forEach((el, index) => { %>
                    <tr>
                        <td><%= index + 1 %></td>
                        <td><%= el.courseName %></td>
                        <td><%= el.price  %></td>
                        <td>
                            <button>See Detail Courses</button>
                        </td>
                    </tr>
                <% }) %>
               
            </tbody>
        </table>
    </section>
</body>
</html>